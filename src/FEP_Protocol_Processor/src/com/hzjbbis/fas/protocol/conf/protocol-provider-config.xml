			</codec>
			<!-- 读编程日志 消息编码/解码器 -->
			<codec fun-code="04">
				<encoder-class>com.hzjbbis.fas.protocol.zj.codec.C04MessageEncoder</encoder-class>
				<decoder-class>com.hzjbbis.fas.protocol.zj.codec.C04MessageDecoder</decoder-class>
			</codec>
			<!-- 实时写对象参数 消息编码/解码器 -->
			<codec fun-code="07">
				<encoder-class>com.hzjbbis.fas.protocol.zj.codec.C07MessageEncoder</encoder-class>
				<decoder-class>com.hzjbbis.fas.protocol.zj.codec.C07MessageDecoder</decoder-class>
			</codec>
			<!-- 写对象参数 消息编码/解码器 -->
			<codec fun-code="08">
				<encoder-class>com.hzjbbis.fas.protocol.zj.codec.C08MessageEncoder</encoder-class>
				<decoder-class>com.hzjbbis.fas.protocol.zj.codec.C08MessageDecoder</decoder-class>
			</codec>
			<!-- 异常告警 消息编码/解码器 -->
			<codec fun-code="09">
				<encoder-class>com.hzjbbis.fas.protocol.zj.codec.C09MessageEncoder</encoder-class>
				<decoder-class>com.hzjbbis.fas.protocol.zj.codec.C09MessageDecoder</decoder-class>
			</codec>						
			<!-- 请求发送端信 消息编码/解码器 -->
			<codec fun-code="40">
				<encoder-class>com.hzjbbis.fas.protocol.zj.codec.C28MessageEncoder</encoder-class>
				<decoder-class>com.hzjbbis.fas.protocol.zj.codec.C28MessageDecoder</decoder-class>
			</codec>
		</codec-factory>
	</protocol-handler>	
	
	
	
</protocol-provider>